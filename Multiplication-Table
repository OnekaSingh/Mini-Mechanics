#include <stdio.h>

void main()
{

    int x, a[11], i; //adjust parameters if needed
    
    x = 10; //multiplier: 10
    
    for(i = 0; i < 11; i++) { //table from 0 to 10
            a[i] = i * x; //multiplies index by multiplier
            printf("%d * %d = %d\n", x, i, a[i]);
    }

}